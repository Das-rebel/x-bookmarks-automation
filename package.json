{
  "name": "x-bookmarks-automation",
  "version": "1.0.0",
  "type": "module",
  "description": "Automated X.com bookmark scraper with API server",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "NODE_ENV=development node server.js",
    "scrape": "node --experimental-json-modules scraper.js",
    "x-bookmarks": "node x-bookmarks.js",
    "migrate:schema": "node migrate-to-consolidated-schema.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "format": "prettier --write .",
    "migrate": "node scripts/run-migrations.js",
    "migrate:create": "node scripts/create-migration.js",
    "pipeline": "node main-pipeline.js",
    "cron": "node cron-pipeline-runner.js",
    "monitor": "node monitoring-system.js",
    "verify-db": "node database-verification.js",
    "ai-process": "node ai-bookmark-processor.js",
    "bulk-fetch": "node bulk-bookmark-fetcher.js",
    "robust-scrape": "node robust-bookmark-scraper.js",
    "simple-scrape": "node simple-bookmark-scraper.js",
    "process-manual": "node process-manual-extraction.js",
    "web-login-scrape": "node src/scrapers/web-login-scraper.js",
    "web-login-simple": "node web-login-scraper-simple.js",
    "scrape-with-auth": "node run-scraper-with-credentials.js",
    "retry-save": "node retry-save-bookmarks.js",
    "save-locally": "node save-bookmarks-locally.js",
    "start:ngrok": "node start-with-ngrok.js",
    "ngrok:setup": "node setup-ngrok.js",
    "ngrok:status": "curl -s http://127.0.0.1:4040/api/tunnels | jq .",
    "process-all": "node process-bookmarks-locally.js",
    "server": "node server.js",
    "dev": "NODE_ENV=development node server.js",
    "sync-supabase": "node sync-to-supabase.js",
    "supabase:test": "node -e \"import('./sync-to-supabase.js').then(m => m.default).then(SyncManager => new SyncManager().testSupabaseConnection())\"",
    "inspect-supabase": "node inspect-supabase-database.js",
    "generate-ai-analysis": "node generate-missing-ai-analysis.js",
    "cleanup": "node cleanup-and-optimize.js",
    "deploy:production": "bash scripts/deploy-production.sh",
    "cron:start": "node scripts/cron-manager.js",
    "cron:stop": "pkill -f cron-manager.js",
    "cron:status": "curl -s http://localhost:3000/cron/status",
    "health": "curl -s http://localhost:3000/health",
    "monitor": "node src/utils/health-monitor.js",
    "test:production": "npm run health && npm run cron:status"
  },
  "dependencies": {
    "@supabase/postgrest-js": "^1.21.2",
    "@supabase/supabase-js": "^2.55.0",
    "axios": "^1.10.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "inquirer": "^8.2.5",
    "jsdom": "^26.1.0",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "puppeteer": "^22.15.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "winston": "^3.11.0"
  },
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Das-rebel/x-bookmarks-automation.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Das-rebel/x-bookmarks-automation/issues"
  },
  "homepage": "https://github.com/Das-rebel/x-bookmarks-automation#readme"
}
